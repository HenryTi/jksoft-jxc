Biz Subject storage '库存' {
    assign goodsInit DEC;
    assign goodsBalance DEC History;
};

Biz Subject c {
    Assign 流水 '下一步' char = 's2';
}

Biz Subject AccountSetting {
    Prop name '单位名称' char;
    Prop 库存上限 dec;
    Assign 库存下限 dec;
};

Biz Subject PersonSetting {
    Prop name '名字' char;
    Prop 工时上限 int;
    Assign 工时下限 int;
};

Biz Subject Price {
    Assign Retail DEC;
};

Biz Subject Sum {
    Assign pSource INT;
    Assign pSource1 INT;
};
