
IDX Book (
    id,
    init VALUE DEFAULT 0,
    value Value,
);
-- Subject 应该也会去掉
ENUM SubjectType (
    Storage = 201,
);

ID Subject (
    id,
    KEY type ENUM SubjectType,
    KEY item ID,
);

ID History (
    id MINUTE,
    subject ID,         -- 科目
    value Value,
    ref ID,             -- 相关凭证
    INDEX subject_id(subject, id) UNIQUE,
);

IDX Done (
    id,
    value VALUE,
);

-- lock some number
IDX Lock (
    id,
    value VALUE,
    validTo TIMESTAMP,      -- 有效至
);
